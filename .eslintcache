')) ? JSON.parse(localStorage.getItem('user')) : ''\r\n\r\n        return ( \r\n            <Layout className='app'>\r\n                <BackTop></BackTop>\r\n                {/* 左边侧边栏 */}\r\n                <AppAside menuToggle={menuToggle} menu={this.state.menu}></AppAside>\r\n                {/* 右边的内容 */}\r\n                <Layout style={{marginLeft: menuToggle ? '80px' : '200px', minHeight: '100vh'}}>\r\n                    {/* 头部组件 */}\r\n                    <AppHeader \r\n                        menuClick={menuClick}\r\n                        loginOut={this.loginOut}\r\n                        menuToggle={menuToggle}\r\n                     ></AppHeader>\r\n\r\n                     {/* 页面主题根据路由渲染页面！！！ */}\r\n                     <Content className='content'>\r\n                         <Switch>\r\n                            {routes.map(item =>{\r\n                                return (\r\n                                    <Route\r\n                                    key={item.path}\r\n                                    path={item.path}\r\n                                    exact={item.exact}\r\n                                    render={props =>\r\n                                        !auth ? (\r\n                                            <item.component {...props} />\r\n                                        ) : item.auth && item.auth.indexOf(auth) !== -1 ? (\r\n                                            <item.component {...props} />\r\n                                        ) : (\r\n                                            // 这里也可以跳转到 403 页面\r\n                                            <Redirect to='/404' {...props} />\r\n                                        )\r\n                                    }></Route> \r\n                                )\r\n                            })}\r\n                            <Redirect to='/404' />\r\n                         </Switch>\r\n                     </Content>\r\n                </Layout>\r\n            </Layout>\r\n         );\r\n    }\r\n}\r\nconst stateToProp = state => ({\r\n    menuToggle: state.menuToggle\r\n})\r\n\r\nconst dispatchToProp = dispatch => ({\r\n    menuClick() {\r\n        dispatch(menuToggleAction())\r\n    }\r\n})\r\n \r\nexport default withRouter(\r\n    connect(\r\n        stateToProp,\r\n        dispatchToProp\r\n    )(DefaultLayout)\r\n)\r\n","C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\store\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\store\\reducer.js",["111"],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\store\\actionType.js",[],["112","113"],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Others\\500\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Others\\500\\500.jsx",[],["114","115"],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Login\\Login.jsx",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Login\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\containers\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\routes\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\containers\\AppAside.jsx",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\components\\CustomMenu\\index.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\components\\CustomMenu\\CustomMenu.jsx",["116","117"],"\r\n// 导航封装主要是在这\r\nimport React, { Component } from 'react';\r\nimport { Menu} from 'antd'\r\nimport { Link, withRouter } from 'react-router-dom'\r\nimport SubMenu from 'antd/lib/menu/SubMenu';\r\nimport MenuItem from 'antd/lib/menu/MenuItem';\r\n\r\nclass CustomMenu extends Component {\r\n    state = { \r\n        openKeys: [], // 打开的导航\r\n        selectedKeys: [] // 选中的值\r\n     }\r\n\r\n     onOpenChange = (openKeys) =>{\r\n        //  console.log(openKeys, '打开了')\r\n         if (openKeys.length === 0 || openKeys.length === 1) {\r\n             this.setState({\r\n                openKeys\r\n             })\r\n             return \r\n         }\r\n\r\n        //   最新展开的 SubMenu\r\n        const latestOpenKey = openKeys[openKeys.length - 1]\r\n\r\n        // 这里与定义的路由规则有关 这里不懂\r\n        if (latestOpenKey.includes(openKeys[0])) {\r\n            this.setState({\r\n                openKeys\r\n            })\r\n        } else {\r\n            this.setState({\r\n                openKeys: [latestOpenKey]\r\n            })\r\n        }\r\n    }\r\n\r\n     // 循环遍历一级导航，生成menu\r\n     renderSubMenu = ({ key, title, subs }) => {\r\n        return (\r\n            <Menu.SubMenu\r\n                key={key}\r\n                title={\r\n                    <span>\r\n                        {title}66\r\n                    </span>\r\n                }\r\n                >\r\n               \r\n               {/* subs 一级导航下面的数组，如果有的话，就加载二级导航， */}\r\n               {/* 没有的话，就加载本身 */}\r\n\r\n                {subs && subs.map(item => {\r\n                    return item.subs && item.subs.length > 0 ? this.renderSubMenu(item) : this.renderMenuItem(item)\r\n                })}\r\n\r\n            </Menu.SubMenu>\r\n        )\r\n     }\r\n\r\n    // 循环二级导航\r\n    renderMenuItem = ({ key, title}) => {\r\n        // console.log(key, '当前的key值')\r\n        return (\r\n            <Menu.Item key={key}>\r\n                {/* link进行路由跳转 */}\r\n                <Link to={key}>\r\n                    <span>{title}12</span>  \r\n                </Link>\r\n            </Menu.Item>\r\n        )\r\n    }\r\n    render() { \r\n        let { openKeys, selectedKeys } = this.state\r\n        return ( \r\n            <Menu\r\n             mode='inline'\r\n             theme='dark'\r\n             openKeys={openKeys}\r\n             selectedKeys={selectedKeys}\r\n             onClick={({ key }) => this.setState({selectedKeys: [key]})}\r\n             onOpenChange={this.onOpenChange}\r\n            >\r\n                {this.props.menu && this.props.menu.map(item => {\r\n                    return item.subs && item.subs.length > 0 ? this.renderSubMenu(item) : this.renderMenuItem(item)\r\n                })}\r\n            \r\n\r\n            </Menu>\r\n       );\r\n    }\r\n}\r\n \r\nexport default withRouter(CustomMenu);\r\n\r\n\r\n// 梳理逻辑\r\n// 根据固定的导航的数据进行遍历，先遍历一级的，如果一级下面有数组的话，\r\n// 就渲染二级导航\r\n\r\n// 没有二级导航的话，就直接走渲染MenuItem 方法\r\n// 有二级导航的话，就走一级导航渲染，之后再根据一级导航判断是否有二级导航\r\n\r\n// DOM结构\r\n\r\n// <Menu>\r\n//     <SubMenu>\r\n//         <MenuItem></MenuItem>\r\n//     </SubMenu>\r\n// </Menu>","C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\containers\\menu.js",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\containers\\AppHeader.jsx",["118","119"],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\PublicView\\Button\\Button.jsx",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Index\\Index.jsx",[],"C:\\Users\\contact\\Desktop\\react-demo\\my-app\\src\\views\\Index\\index.js",[],{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"120","replacedBy":"124"},{"ruleId":"122","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":18,"column":1,"nodeType":"128","endLine":23,"endColumn":2},{"ruleId":"129","severity":1,"message":"130","line":5,"column":27,"nodeType":"131","messageId":"132","endLine":5,"endColumn":34},{"ruleId":"126","severity":1,"message":"127","line":7,"column":1,"nodeType":"128","endLine":14,"endColumn":2},{"ruleId":"120","replacedBy":"133"},{"ruleId":"122","replacedBy":"134"},{"ruleId":"120","replacedBy":"135"},{"ruleId":"122","replacedBy":"136"},{"ruleId":"129","severity":1,"message":"137","line":6,"column":8,"nodeType":"131","messageId":"132","endLine":6,"endColumn":15},{"ruleId":"129","severity":1,"message":"138","line":7,"column":8,"nodeType":"131","messageId":"132","endLine":7,"endColumn":16},{"ruleId":"129","severity":1,"message":"139","line":4,"column":52,"nodeType":"131","messageId":"132","endLine":4,"endCo